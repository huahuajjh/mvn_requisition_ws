<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
						http://www.springframework.org/schema/context
						http://www.springframework.org/schema/context/spring-context-3.0.xsd">
		
		<!-- 资源dao -->
		<bean id="resDao"  class="com.tq.dao.ResDao"/>
		
		<!-- 日志记录dao -->
		<bean id="recordDao"  class="com.tq.dao.OperationRecordDao"/>
		
		<!-- 综合查询dao -->
		<bean id="fmlItemDao"  class="com.tq.dao.FamilyItemDao"/>
		
		<!-- 一键查询dao -->
		<bean id="oneKeyDao"  class="com.tq.dao.OneKeyQueryDao"/>
		
		<!-- 信访dao -->
		<bean id="visitsDao"  class="com.tq.dao.VisitsDao"/>
		
		<!-- 文件类型dao -->
		<bean id="fileTypeDao"  class="com.tq.dao.FileTypeDao"/>
		
		<!-- 档案文件dao -->
		<bean id="fileDao"  class="com.tq.dao.ArchiveFileDao"/>
		
		<!-- 项目分类dao -->
		<bean id="proCategoryDao"  class="com.tq.dao.ProCategoryDao"/>
		
		<!-- 已迁户dao -->
		<bean id="removedInfoDao"  class="com.tq.dao.RemovedInfoDao"/>
		
		<!-- index page dao -->
		<bean id="indexDataDao"  class="com.tq.dao.IndexDataDao"/>
		
		<!-- statistics page dao -->
		<bean id="statistics"  class="com.tq.dao.StatisticsDao"/>
		
		<!-- addr provider dao -->
		<bean id="addrProvider"  class="com.tq.dao.AddrProvider"/>
</beans>